.setting {
    margin: 50px 40px;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}

.logo {
    width: 80px;
    height: 80px;
    border-radius: 50%;
    overflow: hidden;
    margin-bottom: 20px;
}

.name {
    font-size: 15px;
    color: #333;
    margin-bottom: 5px;
}

.content {
    font-size: 14px;
    color: grey;
}

.fab {
    width: 100%;
    display: flex;
    justify-content: center;
}

.change {
    position: fixed;
    bottom: 125px;
    font-size: 16px;
    width: 40%;
    border: 1px solid #00B26A;
    background-color: #00B26A;
    color: #fff;
}

.clearall {
    position: fixed;
    bottom: 70px;
    font-size: 16px;
    width: 40%;
    border: 1px solid #00B26A;
    background-color: #fff;
    color: #00B26A;
}

.loginbtn {
    position: fixed;
    bottom: 70px;
    font-size: 16px;
    width: 40%;
    border: 1px solid #00B26A;
    background-color: #00B26A;
    color: #fff;
}

.inputmodal {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    overflow: hidden;
    margin-top: 30%;
}